{
  "name": "@gurinder/vue-collapse",
  "version": "1.0.0",
  "private": false,
  "author": "Gurinder Chauhan <gurinderchauhan@gmail.com>",
  "homepage": "https://github.com/gurindersingh/vue-collapse",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "pack": "vue-cli-service build --target lib --name VueCollapse ./src/components/index.js"
  },
  "main": "dis/VueCollapse.common.js",
  "files": [
    "dist/*.js",
    "dist/*.js.map",
    "src/*",
    "public/*",
    "*.json",
    "*.js",
    "*.md"
  ],
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}